{
  "name": "serverless-rest-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for creating API's with the Serverless framework and AWS. Provisions Lambda, API Gateway, and DynamoDB with help from a few useful tools/frameworks",
  "scripts": {
    "deploy": "sls deploy",
    "offline": "source offline.sh && sls offline start",
    "install": "sls dynamodb install",
    "lint": "eslint --fix \"src/**/*.js\" && prettier --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baileytincher/serverless-REST-boilerplate.git"
  },
  "author": "Bailey Tincher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baileytincher/serverless-REST-boilerplate/issues"
  },
  "homepage": "https://github.com/baileytincher/serverless-REST-boilerplate#readme",
  "dependencies": {
    "@middy/core": "^1.0.0-alpha.34",
    "@middy/http-cors": "^1.0.0-alpha.34",
    "@middy/http-error-handler": "^1.0.0-alpha.34",
    "@middy/http-json-body-parser": "^1.0.0-alpha.34",
    "@middy/validator": "^1.0.0-alpha.34",
    "bcryptjs": "^2.4.3",
    "dynamoose": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-source-map-support": "^2.0.1",
    "eslint": "^6.0.1",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^6.0.0",
    "prettier": "^1.18.2",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.7.3",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2"
  }
}

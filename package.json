{
  "name": "serverless-typescript-rest-boilerplate",
  "version": "1.0.0",
  "description": "A Typescript boilerplate for creating REST API's with the Serverless Framework and Middy",
  "scripts": {
    "offline": "sls offline start",
    "lint": "eslint 'src/**/*.{js,ts}' --fix",
    "commit": "npm run lint && git add . && git commit && git push",
    "start": "npm run offline",
    "deploy": "sls deploy",
    "ci": "npm run commit && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baileytincher/serverless-REST-boilerplate.git"
  },
  "author": "Bailey Tincher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baileytincher/serverless-REST-boilerplate/issues"
  },
  "homepage": "https://github.com/baileytincher/serverless-REST-boilerplate/#readme",
  "dependencies": {
    "@middy/core": "^1.0.0",
    "@middy/do-not-wait-for-empty-event-loop": "^1.0.0",
    "@middy/http-cors": "^1.0.0",
    "@middy/http-json-body-parser": "^1.0.0",
    "@middy/validator": "^1.0.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/node": "^13.13.5",
    "ajv": "^6.12.3",
    "ajv-keywords": "^3.4.1",
    "google-auth-library": "^6.0.0",
    "http-errors": "^1.7.3",
    "mongoose": "^5.9.13",
    "safe-buffer": "^5.2.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "serverless": "^1.70.0",
    "serverless-domain-manager": "^4.0.0",
    "serverless-offline": "^6.1.5",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "^5.3.2",
    "ts-loader": "^7.0.3",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2"
  }
}
